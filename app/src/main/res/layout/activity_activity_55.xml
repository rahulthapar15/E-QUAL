<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.rahulthapar.e_qual.Activity_47_50">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.example.rahulthapar.e_qual.Activity_47_50">

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/radiogroup55">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="If you have not taken any online course, would you consider taking an online course? "
            android:id="@+id/tv1_55"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Yes"
            android:id="@+id/yes55"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="No"
            android:id="@+id/no55"/>
        </RadioGroup>
        <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/radiogroup56"
            android:layout_below="@+id/radiogroup55"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Does those courses help you to enhance your understanding?"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/tv2_55"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Yes"
            android:id="@+id/yes56"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="No"
            android:id="@+id/no56"/>
</RadioGroup>
        <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/radiogroup57"
            android:layout_below="@+id/radiogroup56"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Do you think online module in particular topic in your subject by renowned professional will be helpful?"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/tv3_55"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Yes"
            android:id="@+id/yes57"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="No"
            android:id="@+id/no57"
            android:checked="false" />
    </RadioGroup>
    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/radiogroup58"
        android:layout_below="@+id/radiogroup57"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Which one of the following will be prefer receiving teachingtaking the course through "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/tv4_55"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Computer"
            android:id="@+id/radiobutton1_55"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mobile"
            android:id="@+id/radiobutton2_55"/>
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Both"
            android:id="@+id/radiobutton3_55"
            android:checked="false" />
        <RadioButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="None"
            android:id="@+id/radiobutton4_55"/>
    </RadioGroup>
        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="FINSIH"
            android:id="@+id/save_next_act55"
            android:layout_gravity="end"
            android:background="@color/colorPrimary"
            android:layout_below="@+id/radiogroup58"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:id="@+id/cancel_55"
        android:layout_gravity="end"
        android:background="@color/colorPrimary"
            android:layout_alignTop="@+id/save_next_act55"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/bright_foreground_disabled_material_dark"/>
</RelativeLayout>
    </ScrollView>
